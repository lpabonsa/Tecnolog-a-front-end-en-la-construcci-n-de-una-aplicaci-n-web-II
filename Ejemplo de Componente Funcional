// Home.js
import React, { useContext } from 'react';
import MyContext from '../MyContext';

const Home = () => {
  const { data } = useContext(MyContext);

  return (
    <div>
      <h2>PÃ¡gina de Inicio</h2>
      <p>Datos desde el servidor:</p>
      <ul>
        {data.map(item => (
          <li key={item.id}>{item.name}</li>
        ))}
      </ul>
    </div>
  );
};

export default Home;
